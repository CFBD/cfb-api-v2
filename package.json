{
  "name": "cfb-api",
  "version": "5.5.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/CFBD/cfb-api-v2.git"
  },
  "private": true,
  "main": "src/app.ts",
  "scripts": {
    "build": "tsoa spec-and-routes && tsc --skipLibCheck",
    "build:db": "yarn kysely-codegen --camel-case --out-file=db.d.ts",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "start": "node ./build/app.js",
    "test": "jest",
    "lint": "eslint  --ignore-path .gitignore .",
    "lint:fix": "eslint --ignore-path .gitignore . --fix",
    "prettier-check": "prettier -c --ignore-path .gitignore \"**/*.{css,html,js,json,md,yaml,yml,ts}\"",
    "prettier-check:updated": "pipe-git-updated --base=main --ext=css --ext=html --ext=js --ext=ts --ext=json --ext=md --ext=yaml --ext=yml -ext=js --ext=mdx -- prettier -c",
    "prettify": "prettier --write --ignore-path .gitignore \"**/*.{css,html,js,json,yaml,yml,ts,tsx,jsx,vue,}\" -c",
    "prepare": "husky"
  },
  "keywords": [
    "football",
    "college",
    "cfb",
    "ncaaf",
    "data",
    "statistics"
  ],
  "author": "BlueSCar",
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^7.113.0",
    "axios": "^1.7.4",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "gaussian": "^1.3.0",
    "helmet": "^7.1.0",
    "kysely": "^0.27.3",
    "pg": "^8.12.0",
    "pg-promise": "^11.6.0",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.2.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.4",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@jest-mock/express": "^2.1.0",
    "@jest/globals": "^29.7.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/bluebird": "^3.5.42",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/gaussian": "^1.2.2",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.8",
    "@types/passport": "^1.0.16",
    "@types/pg": "^8.11.6",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.2.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "joi": "^17.13.1",
    "kysely-codegen": "^0.15.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "semantic-release": "^24.1.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
